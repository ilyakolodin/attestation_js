{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": false,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "allowJs": true,
        "jsx": "react",
        "lib": ["es5", "dom"],
        "types": ["cypress", "node"],
    },
    "include": [
        "./src/**/*",
        "./cypress/**/*"
    ]
}